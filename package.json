{
    "name": "ravgeet-web",
    "version": "1.0.0",
    "description": "This is a NPM package manager file for Ravgeet Dhillon's website.",
    "scripts": {
        "setup": "chmod +x _scripts/bash/setup.sh && _scripts/bash/setup.sh",
        "compile-css": "chmod +x _scripts/bash/bundle-css.sh && _scripts/bash/bundle-css.sh",
        "minify-html": "chmod +x _scripts/bash/minify-html.sh && _scripts/bash/minify-html.sh",
        "develop": "bundle exec jekyll serve --host localhost",
        "build": "chmod +x _scripts/bash/build.sh && _scripts/bash/build.sh"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ravgeetdhillon/ravgeet-web"
    },
    "bugs": {
        "url": "https://github.com/ravgeetdhillon/ravgeet-web/issues"
    },
    "homepage": "https://github.com/ravgeetdhillon/ravgeet-web",
    "keywords": [],
    "author": "Ravgeet Dhillon",
    "license": "ISC",
    "dependencies": {
        "bootstrap": "4.6.0"
    },
    "devDependencies": {
        "html-minifier": "4.0.0",
        "node-sass": "5.0.0"
    }
}
